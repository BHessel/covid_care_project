<h1>Add Your Items</h1>

<ul>
    <% @items.each do |item| %>
    <%=form_for item do |f| %> 
        <%= f.label :name, item.name %>
        <%= f.check_box :name %>
        <% end %>
    <% end %>
    <div>
        <button><%= link_to "Checkout", packages_add_items_path %></button>
    </div>
</ul>